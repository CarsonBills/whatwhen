<script>

  $(document).ready(function() {
    $('#calendar').fullCalendar({
      events: {
      	url: "/events/<%= @events %>",
        },
      header: {
      	left: "month, agendaWeek, agendaDay",
      	center: "title",
      	right: "prevYear,prev,today,next,nextYear"
      }
  	})
  });
</script>

<%= link_to 'New Event', new_event_path %>

<div id="calendar">
</div>